{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "valid8-ui-prototype/dist",
  "installCommand": "cd valid8-ui-prototype && npm install",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/webhooks/stripe.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/create-checkout-session.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api/webhooks/stripe",
      "destination": "/api/webhooks/stripe.py"
    },
    {
      "source": "/api/create-checkout-session",
      "destination": "/api/create-checkout-session.py"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/valid8-ui-prototype/dist/assets/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/valid8-ui-prototype/dist/index.html"
    }
  ]
}
